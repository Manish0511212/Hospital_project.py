import sqlite3
import pandas as pd

def display_table(table_name):
    conn = sqlite3.connect("hospital.db")
    df = pd.read_sql(f"SELECT * FROM {table_name}", conn)
    print(f"\nðŸ”¹ {table_name} Table:")
    print(df)
    conn.close()
    return df

tables = ["Patients", "Doctors", "Appointments", "Bills", "Beds"]
for table in tables:
    display_table(table)
